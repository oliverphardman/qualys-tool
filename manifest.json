{
	"name": "Qualys Tool",
	"description": "A tool to make Qualys life easier.",
	"version": "1.0",
	"manifest_version": 2,
	"author": "Oliver Hardman/VRM @ Telefonica",
	"icons": {
		"16": "icons/16.png",
		"32": "icons/32.png",
		"48": "icons/48.png",
		"64": "icons/64.png",
		"128": "icons/128.png",
		"256": "icons/256.png"
	},
	"permissions": ["https://qualysapi.qualys.com/", "https://qualys.com/", "storage"],
	"browser_action": {
		"default_popup": "initial.html",
		"default_title": "Qualys Tool",
		"default_icon": {
			"16": "icons/16.png",
			"32": "icons/32.png",
			"48": "icons/48.png",
			"64": "icons/64.png",
			"128": "icons/128.png",
			"256": "icons/256.png"
		}
	}
}
